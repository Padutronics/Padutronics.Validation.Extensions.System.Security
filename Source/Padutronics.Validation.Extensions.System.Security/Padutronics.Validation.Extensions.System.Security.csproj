<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningLevel>5</WarningLevel>
        <Version>0.0.0</Version>
        <Authors>Pavel Dubsky</Authors>
        <Company>Padutronics</Company>
        <Product>Padutronics Framework</Product>
        <Description>Validation extensions for System.Security namespace.</Description>
        <Copyright>Copyright Â© Padutronics 2023</Copyright>
        <PackageProjectUrl>https://github.com/Padutronics/Padutronics.Validation.Extensions.System.Security</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Padutronics/Padutronics.Validation.Extensions.System.Security</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Padutronics.Extensions.System.Security" Version="0.1.0" />
        <PackageReference Include="Padutronics.Validation" Version="0.1.0" />
    </ItemGroup>
</Project>